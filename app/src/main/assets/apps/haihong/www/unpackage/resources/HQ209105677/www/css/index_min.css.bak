.jumbotron {
    position: relative;
    width: 100%;
    color: #fff;
    background: -webkit-linear-gradient(top left,#fe714d,#f52a3b);
    background: -o-linear-gradient(top left,#fe714d,#f52a3b);
    background: linear-gradient(to bottom right,#fe714d,#f52a3b);
    overflow: hidden;
    box-shadow: 0 3px 5px 0 rgba(238,113,108,.6)
}

.jumbotron:before {
    content: '';
    display: block;
    width: 3.1250000100rem;
    height: 3.1250000100rem;
    background-color: rgba(255,255,255,.07);
    position: absolute;
    top: -.9375000030rem;
    left: -.9375000030rem;
    border-radius: 100%;
    z-index: 0;
    -webkit-animation: jumbotron-circle 20s infinite forwards linear
}

.jumbotron:after {
    content: '';
    display: block;
    width: 3.50000112rem;
    height: 3.50000112rem;
    background-color: rgba(255,255,255,.07);
    position: absolute;
    top: 0;
    right: -1.5625000050rem;
    border-radius: 100%;
    z-index: 0;
    -webkit-animation: jumbotron-circle 30s infinite forwards linear
}

@-webkit-keyframes jumbotron-circle {
    0% {
        -webkit-transform: translate3d(0,0,0)
    }

    25% {
        -webkit-transform: translate3d(0,20px,20px)
    }

    50% {
        -webkit-transform: translate3d(-20px,0,20px)
    }

    75% {
        -webkit-transform: translate3d(-20px,20px,0)
    }

    100% {
        -webkit-transform: translate3d(0,0,0)
    }
}

