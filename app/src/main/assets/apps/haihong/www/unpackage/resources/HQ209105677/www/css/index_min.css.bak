@-webkit-keyframes head-shake {
    0% {
        -webkit-transform: rotate(0)
    }

    25% {
        -webkit-transform: rotate(8deg)
    }

    50% {
        -webkit-transform: rotate(0)
    }

    75% {
        -webkit-transform: rotate(-8deg)
    }

    100% {
        -webkit-transform: rotate(0)
    }
}

@-webkit-keyframes start {
    0% {
        opacity: 1;
        -webkit-transform: scale(1)
    }

    25% {
        opacity: .2;
        -webkit-transform: scale(.9)
    }

    50% {
        opacity: 1;
        -webkit-transform: scale(1)
    }

    75% {
        opacity: .2;
        -webkit-transform: scale(.9)
    }

    100% {
        opacity: 1;
        -webkit-transform: scale(1)
    }
}

@-webkit-keyframes notice-hide {
    0% {
        opacity: 1;
        -webkit-transform: scale(1)
    }

    70% {
        opacity: 1;
        -webkit-transform: scale(.08);
        top: 47px;
        left: 40px
    }

    100% {
        opacity: 0;
        -webkit-transform: scale(.08);
        top: 47px;
        left: 40px
    }
}

@-webkit-keyframes login-alert-hide {
    0% {
        opacity: 1
    }

    100% {
        opacity: 0
    }
}

