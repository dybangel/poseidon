        opacity: 0
    }

    100% {
        opacity: 0
    }
}

@keyframes ring {
    0% {
        -webkit-transform: scale(1);
        opacity: 1
    }

    80% {
        -webkit-transform: scale(2.2);
        opacity: 0
    }

    100% {
        opacity: 0
    }
}

@-webkit-keyframes move {
    0% {
        right: -1rem;
        bottom: -1.2rem
    }

    50% {
        right: -.7rem;
        bottom: -1.2rem
    }

    100% {
        right: -1rem;
        bottom: -1.2rem
    }
}

